/*
函数作用：求两个数的最大公约数
参数：
参数一；其中一个参数
参数二：另外一个参数
*/
int GCD_1(int x,int y)
{
	int min_t = x>y?x:y;
	for(;;min_t--)
	{
		if(x%min_t==0 && y%min_t==0)
			break;
	}
	return min_t;
}

int GCD_2(int x,int y)
{
	int r;
	while(r!=0)
	{
		r=x%y;
		x=y;
		y=r;
	}
	return x;
}

int GCD_3(int x,int y)
{
	if(x==y)
	{
		return x;
	}
	else if(x>y)
	{
		return GCD_3(x-y,y);
	}
	else
	{
		return GCD_3(x,y-x);
	}
}
